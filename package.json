    "istanbul": "^0.4.5",
    "mocha": "^4.0.0",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-require": "^2.0.2",
    "node-env-file": "^0.1.8",
